{
    "description": "Light Interface Maker Application",
    "homepage": "https://github.com/yanickmorza/lima",
    "name": "yanickmorza/lima",
    "type": "project",
    "license": "MIT",
    "keywords": ["generator", "code generator", "scaffolding", "scaffold"],
    "authors": [
        {
            "name": "Yanick Morza",
            "email": "yanick.morza@free.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.5"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\Lima\\": "src/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php vendor/yanickmorza/lima/src/Command/lima-ci.php",
            "@auto-scripts"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}